{
  "name": "electron-app-market-history-scraper",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "keywords": [],
  "author": "jaki≈õ random debil",
  "license": "UNLICENSED",
  "main": "dist-electron/main.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "ts:check": "npx tsc --noEmit --extendedDiagnostics ",
    "tsgo:check": "npx tsgo --noEmit --extendedDiagnostics ",
    "db:generate": "drizzle-kit generate --config electron/core/db/drizzle.config.ts",
    "db:push": "drizzle-kit push --config electron/core/db/drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config electron/core/db/drizzle.config.ts",
    "db:studio": "drizzle-kit studio --port=5001 --config electron/core/db/drizzle.config.ts",
    "pack": "npm run build && electron-builder --dir",
    "pack:win": "npm run build && electron-builder --win --dir",
    "pack:mac": "npm run build && electron-builder --mac --dir",
    "pack:linux": "npm run build && electron-builder --linux --dir",
    "dist": "npm run build && electron-builder",
    "dist:win": "npm run build && electron-builder --win",
    "dist:win:nsis": "npm run build && electron-builder --win nsis",
    "dist:win:portable": "npm run build && electron-builder --win portable",
    "dist:win:msi": "npm run build && electron-builder --win msi",
    "dist:mac": "npm run build && electron-builder --mac",
    "dist:mac:dmg": "npm run build && electron-builder --mac dmg",
    "dist:linux": "npm run build && electron-builder --linux",
    "dist:linux:appimage": "npm run build && electron-builder --linux AppImage"
  },
  "dependencies": {
    "@libsql/client": "^0.17.0",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.45.1",
    "lucide-react": "^0.575.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-router-dom": "^7.13.0",
    "tailwind-merge": "^3.5.0"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.2.0",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@typescript/native-preview": "^7.0.0-dev.20260220.1",
    "@vitejs/plugin-react": "^5.1.4",
    "drizzle-kit": "^0.31.9",
    "electron": "^40.6.0",
    "electron-builder": "^26.8.1",
    "tailwindcss": "^4.2.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-static-copy": "^3.2.0",
    "vite-tsconfig-paths": "^6.1.1"
  },
  "overrides": {
    "esbuild": "^0.27.3",
    "esm-loader": "tsx@^4.21.0",
    "minimatch": "10.2.2"
  },
  "packageManager": "npm@11.6.2"
}
